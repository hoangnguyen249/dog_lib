
<table class="table">
  <thead>
    <tr>
      <th scope="col">Num</th>
      <th scope="col">Name</th>
      <th scope="col">Image</th>
      <th scope="col">Owner</th>
    </tr>
  </thead>
  <tbody>
  <%  i=1 %>
  <% Breed.all.each do |breed| %>
  <tr>
      <th scope="row"><%=i %></th>
      <td><%= breed.id %></td>
      <td><%= link_to breed.name, info_path(id: breed.id) %></td>
      <td> <%= image_tag breed.images.first.url, size: '200x200' %></td>
      <td>
  <% breed.owners.each do |owner| %>
    <%= owner.name %><br>
  <% end %>
</td>

      <%i+=1 %>
    </tr>
  <% end %>
  </tbody>
</table>


