<table class="table">
  <thead>
    <tr>
      <th scope="col">Num</th>
      <th scope="col">Name</th>
      <th scope="col">Image</th>
      <th scope="col">Owner</th>
    </tr>
  </thead>
  <tbody>
  <%  i=1 %>
  <% Breed.all.each do |breed| %>
  <tr>
      <th scope="row"><%=i %></th>
      <td><%= breed.name %></td>
      <td> <%= image_tag breed.images.first.url, size: '200x200' %></td>
      <td><%= breed.owners.first.name%> </td>
      <%i+=1 %>
    </tr>
  <% end %>
  </tbody>
</table>


