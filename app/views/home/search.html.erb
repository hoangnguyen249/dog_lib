<h1>Search Results for "<%= @query %>"</h1>

<% if @breeds.present? %>
  <ul>
    <% @breeds.each do |breed| %>
      <li><%= breed.name %></li>
      <li><%= breed.gender %></li>
      <li><%= breed.size %></li>
      <!-- Display other breed information as needed -->
      <br>
    <% end %>
  </ul>
<% else %>
  <p>No results found for "<%= @query %>".</p>
<% end %>
