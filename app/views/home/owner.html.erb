<table class="table">
  <thead>
    <tr>
      <th scope="col">Num</th>
      <th scope="col">Name</th>
      <th scope="col">Breed</th>
    </tr>
  </thead>
  <tbody>
  <%  i=1 %>
  <% Owner.all.each do |owner| %>
  <tr>
      <th scope="row"><%=i %></th>
      <td><%= owner.name %></td>
      <td><%= owner.breeds.first.name %></td>
      <%i+=1 %>
    </tr>
  <% end %>
  </tbody>
</table>

