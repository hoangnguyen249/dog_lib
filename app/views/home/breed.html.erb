<h1>Dog List</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Num</th>
      <th scope="col">Name</th>
      <th scope="col">Gender</th>
      <th scope="col">Size</th>
    </tr>
  </thead>
  <tbody>
  <%  i=1 %>
  <% Breed.all.each do |breed| %>
  <tr>
      <th scope="row"><%=i %></th>
      <td><%= breed.name.capitalize %></td>
      <td> <%= breed.gender.capitalize%></td>
      <td> <%= breed.size.capitalize %></td>
</td>

      <%i+=1 %>
    </tr>
  <% end %>
  </tbody>
</table>


